{
  "mcpServers": {
    "context7": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@upstash/context7-mcp@latest"
      ]
    },
    "sequential-thinking": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "playwright": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "@playwright/mcp@latest"
      ]
    },
    "filesystem": {
      "type": "stdio",
      "command": "cmd",
      "args": [
      "/c", 
      "npx",
      "-y",
      "@modelcontextprotocol/server-filesystem",
      "/Users"
      ]
    },
    "ClaudeTalkToFigma": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "claude-talk-to-figma-mcp@latest"
      ]
    }
  }
}